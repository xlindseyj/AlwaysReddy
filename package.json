{
  "name": "lws-ai-2",
  "version": "1.0.0",
  "description": "First, create a virtual environment, update pip, and install the required packages:",
  "main": "index.js",
  "scripts": {
    "py:venv": "python -m venv .venv",
    "py:activate": ".venv\\Scripts\\activate",
    "py:install": ".venv\\Scripts\\pip install -r requirements/requirements.txt && .venv\\Scripts\\pip install -r requirements/faster_whisper_requirements.txt && .venv\\Scripts\\pip install -r requirements/transformer_whisper_requirements.txt",
    "py:setup": ".venv\\Scripts\\python setup.py",
    "py:dev": ".venv\\Scripts\\python main.py",
    "py:deps": ".venv\\Scripts\\pip list"
  },
